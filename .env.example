# GCP Settings - Get these from your Vertex AI Search setup
PROJECT_ID=your-project-id
LOCATION=global
DATA_STORE_ID=your-datastore-id
APP_ID=your-app-id

# Slack Settings - Get these from your Slack app configuration
SLACK_BOT_TOKEN=xoxb-your-bot-token
SLACK_SIGNING_SECRET=your-signing-secret
SLACK_APP_TOKEN=xapp-your-app-token

# Feature Configuration
USE_GROUNDED_GENERATION=True
MAX_RESULTS=5
RESPONSE_TIMEOUT=30

# Service Account (for local development)
GOOGLE_APPLICATION_CREDENTIALS=./service-account.json

# Optional: Bot Configuration
BOT_NAME=KnowledgeBot
RATE_LIMIT_PER_USER=10
RATE_LIMIT_WINDOW=60